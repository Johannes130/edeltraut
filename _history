use(300)\n\n    if d < 5:   # obstacle very close\n        stop()\n        basic.pause(200)\n\n        # check left\n        left()\n        basic.pause(300)\n        stop()\n        dist_left = ultrasonic_distance_cm()\n\n        # check right\n        right()\n        basic.pause(600)   # rotate further to the right\n        stop()\n        dist_right = ultrasonic_distance_cm()\n\n        # return to forward-facing orientation (undo the extra right turn)\n        left()\n        basic.pause(300)\n        stop()\n\n        # decide\n        if dist_left > 5:\n            left()\n            basic.pause(300)\n        elif dist_right > 5:\n            right()\n            basic.pause(300)\n        else:\n            # both sides blocked → turn around\n            left()\n            basic.pause(600)\n    else:\n        forward()\n\nbasic.forever(on_forever)\n"}]},{"timestamp":1758094325120,"editorVersion":"8.0.16","changes":[{"type":"edited","filename":"main.py","patch":[{"start1":0,"length1":109,"diffs":[[1,"# MakeCode Python (micro:bit) - 2-motor obstacle avoidance with HC-SR04\n# Motors: left -> P13, right -> P14\n# Ultrasonic: TRIG -> P12, ECHO -> P9\n"]]},{"start1":601,"length1":23,"diffs":[[1,"    # send trigger pulse\n"]]},{"start1":820,"length1":187,"diffs":[[1,"    # measure echo pulse (microseconds)\n    duration = pins.pulse_in(DigitalPin.P9, PulseValue.HIGH, 30000)  # 30 ms timeout\n    if duration == 0:\n        return 999             # timeout → no echo (treat as 'far away')\n    # convert µs → cm (typical HC-SR04 formula)\n    distance_cm = duration / 58.0\n    return distance_cm\n"]]},{"start1":1164,"length1":90,"diffs":[[1,"    d = ultrasonic_distance_cm()\n    basic.show_number(int(d))   # quick readout on the LED grid\n    basic.pause(300)\n"]]},{"start1":1283,"length1":17,"diffs":[[1,"    if d < 5:   # obstacle very close\n"]]},{"start1":1362,"length1":23,"diffs":[[1,"        # check left\n"]]},{"start1":1484,"length1":24,"diffs":[[1,"        # ch