me\n\n# Motorpins\n# Linker Motor an P13 (Rückwärts) und P14 (Vorwärts)\n# Rechter Motor an P15 (Rückwärts) und P16 (Vorwärts)\n\ndef forward():\n    pin14.write_analog(1000)  # linker Motor vorwärts\n    pin13.write_digital(0)\n    pin16.write_analog(1000)  # rechter Motor vorwärts\n    pin15.write_digital(0)\n\ndef left():\n    pin14.write_digital(0)\n    pin13.write_digital(0)\n    pin16.write_analog(1000)  # nur rechter Motor vorwärts\n    pin15.write_digital(0)\n\ndef stop():\n    pin13.write_digital(0)\n    pin14.write_digital(0)\n    pin15.write_digital(0)\n    pin16.write_digital(0)\n\n# Ultraschallmessung\ndef get_distance():\n    pin12.write_digital(0)\n    utime.sleep_us(2)\n    pin12.write_digital(1)\n    utime.sleep_us(10)\n    pin12.write_digital(0)\n\n    while pin9.read_digital() == 0:\n        pulse_start = utime.ticks_us()\n    while pin9.read_digital() == 1:\n        pulse_end = utime.ticks_us()\n\n    duration = pulse_end - pulse_start\n    distance = (duration / 2) / 29.1  # cm\n    return distance\n\n# Hauptprogramm\nwhile True:\n    dist = get_distance()\n    display.scroll(str(int(dist)))\n\n    utime.sleep_ms(500)\n\n    if dist < 5:\n        stop()\n        left()\n        utime.sleep_ms(200)\n        stop()\n    else:\n        forward()\n"]]}]}]},{"timestamp":1758101275109,"editorVersion":"8.0.16","changes":[{"type":"edited","filename":"main.ts","patch":[{"start1":0,"length1":0,"diffs":[[1,"let dist = 0\nfunction right() {\n    pins.digitalWritePin(DigitalPin.P13, 1)\n    pins.digitalWritePin(DigitalPin.P14, 0)\n}\n"]]},{"start1":123,"length1":0,"diffs":[[1,"function left() {\n    pins.digitalWritePin(DigitalPin.P13, 0)\n    pins.digitalWritePin(DigitalPin.P14, 1)\n}\n\nfunction stop() {\n    pins.digitalWritePin(DigitalPin.P13, 0)\n    pins.digitalWritePin(DigitalPin.P14, 0)\n}\n\nfunction ultrasonic_distance_cm(): number {\n    //  Trig-Puls\n    pins.digitalWritePin(DigitalPin.P12, 0)\n    control.waitMicros(2)\n    pins.digitalWritePin